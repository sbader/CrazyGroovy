= form_for(@song) do |f|
  - if @song.errors.any?
    #errorExplanation
      %h2= "#{pluralize(@song.errors.count, 'error')} prohibited this user from being saved:"
      %ul
      - @song.errors.full_messages.each do |msg|
        %li= msg
  
  .field
    = f.label :title
    %br
    = f.text_field :title
  .field
    = f.label :track
    %br
    = f.text_field :track

  .actions
    = f.submit
